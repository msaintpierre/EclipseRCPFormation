<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.sofip.rental.ui.category"
            name="Rental">
      </category>
      <view
            category="com.sofip.rental.ui.category"
            class="com.sofip.rental.ui.views.RentalPropertyView"
            icon="icons/Rentals.png"
            id="com.sofip.rental.ui.rentalPropertyView"
            name="Rental Properties"
            restorable="true">
      </view>
      <view
            category="com.sofip.rental.ui.category"
            class="com.sofip.rental.ui.views.RentalAgencyView"
            icon="icons/Agency.png"
            id="com.sofip.rental.ui.rentalAgencyView"
            name="Rental Agency"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.sofip.rental.ui.perspective.RentalPerspective"
            icon="icons/Agency.png"
            id="com.sofip.rental.ui.perspective"
            name="Rental">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.sofip.rental.ui.prefs.RentalColorPrefs"
            id="com.sofip.rental.ui.colorprefpage"
            name="Colors">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.sofip.rental.ui.prefs.RentalUIPrefInitializer">
      </initializer>
   </extension>

</plugin>
